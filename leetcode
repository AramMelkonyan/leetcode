#592

import re
import math
class Solution:
    def fractionAddition(self, expression):
        tver = list(map(int, re.findall('[+-]?[0-9]+', expression)))
        A = 0
        B = 1
        for a, b in zip(tver[::2], tver[1::2]):
            A = A * b + a * B
            B *= b
            g = math.gcd(A, B)
            A //= g
            B //= g
        return str(A) + '/' + str(B)


expression = "-1/2+1/2+1/3"
print(Solution().fractionAddition(expression)) 